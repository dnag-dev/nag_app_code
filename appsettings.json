{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureAppService": {
    "StartupCommand": "python startup.py",
    "StartupTimeLimit": 120,
    "HealthCheckPath": "/health",
    "HealthCheckInterval": 30,
    "HealthCheckTimeout": 10,
    "HealthCheckUnhealthyThreshold": 3
  }
} 